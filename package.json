{
  "name": "patsabot",
  "version": "3.0.0-alpha.1",
  "author": "Patsagorn Y.",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ptsgrn/patsabot.git"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "module": "src/core/run.ts",
  "dependencies": {
    "@commander-js/extra-typings": "^13.1.0",
    "@elysiajs/bearer": "^1.4.2",
    "@elysiajs/jwt": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@paralleldrive/cuid2": "^2.3.1",
    "biome": "^0.3.3",
    "chalk": "^5.6.2",
    "commander": "^13.1.0",
    "cors": "^2.8.5",
    "croner": "^9.1.0",
    "cronstrue": "^2.61.0",
    "elysia": "^1.4.22",
    "fast-diff": "^1.3.0",
    "moment": "^2.30.1",
    "mwn": "^2.0.4",
    "mysql2": "^3.16.0",
    "wikiparser-node": "^1.33.0",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.19.6",
    "@types/ssh2": "^1.15.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "3.5.3",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  },
  "bugs": {
    "url": "https://github.com/ptsgrn/patsabot/issues"
  },
  "description": "NodeJS for Thai wikipedia",
  "homepage": "https://github.com/ptsgrn/patsabot#readme",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Patsagorn Y.",
      "email": "mpy@toolforge.org",
      "web": "https://w.wiki/JSB"
    }
  ],
  "private": "true",
  "scripts": {
    "start": "/data/project/patsabot/.bun/bin/bun i && /data/project/patsabot/.bun/bin/bun ./src/core/web.ts",
    "prepare": "husky"
  },
  "type": "module",
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true",
      "biome check --write --no-errors-on-unmatched",
      "biome check --write --unsafe --no-errors-on-unmatched",
      "biome format --write --no-errors-on-unmatched",
      "biome lint --write --no-errors-on-unmatched"
    ]
  },
  "trustedDependencies": [
    "core-js",
    "esbuild"
  ]
}
